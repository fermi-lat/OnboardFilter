package OnboardFilter
version v0r0p0

use EbfWriter           *

apply_pattern packageShr
apply_pattern package_include
apply_pattern package_header

apply_pattern library_Llinkopts library="OnboardFilterTds"

private

macro OnboardFilter_cppflags "" \
   Linux "-DGLEAM -D__i386"\
   WIN32 "/D GLEAM /D__i386"

macro OnboardFilter_cflags $(OnboardFilter_cppflags)

macro headers "" WIN32 "*.h"
library OnboardFilterTds -no_share FilterStatus.cxx

library OnboardFilter $(headers) *.cxx *.c Dll/*.cxx

private
apply_pattern package_Cshlibflags

public

apply_pattern package_test
set TESTJOBOPTIONS ${ONBOARDFILTERROOT}/src/test/jobOptions.txt
